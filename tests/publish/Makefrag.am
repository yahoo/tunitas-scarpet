tests_publish_TESTS = \
  tests/publish/all.test \
  $(end)

tests_publish_TESTS_SELFCHECK_ACTUAL   = $(wildcard tests/publish/*.test)
tests_publish_TESTS_SELFCHECK_EXPECTED = $(tests_publish_TESTS)
