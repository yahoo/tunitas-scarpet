#divert <hpp>
#import cppunit.Test
#import cppunit.test.Fixture
#import tests.unit.resolve.aspect.Channel
namespace tests::unit::resolve {
  template<> class Lookup<NA> : public cppunit::test::Fixture, aspect::Channel {
  public:
    auto setUp() -> void final { Channel::setUp(); }
    auto tearDown() -> void final { Channel::tearDown(); }
