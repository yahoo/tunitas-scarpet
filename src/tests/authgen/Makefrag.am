check_bin_authgen = check/bin/authgen
check_bin_authgen_LDADD = \
  $(lib_libapp_authgen_la) \
  $(check_lib_libtests_run_la) \
  $(check_lib_libtests_la) \
  -lrigging-bench-module \
  $(end)
$(check_bin_authgen_OBJECTS): AM_CPPFLAGS+=$(AND_CHECK_CPPFLAGS_SET)
$(check_bin_authgen_OBJECTS): AM_CXXFLAGS+=$(AND_CHECK_CXXFLAGS_SET) -Wno-deprecated-declarations
# reminder: autotools does not put $(AM_LDFLAGS) into check_bin_authgen_LDFLAGS, like it does with CPPFLAGS and CXXFLAGS
check_bin_authgen_LDFLAGS = $(CHECK_LDFLAGS_SET)
check_bin_authgen_MODULES = $(call TF_SOURCEStoMODULES, $(check_bin_authgen_SOURCES))
check_bin_authgen_SOURCES = \
  obj/src/tests/authgen/namespace.cpp \
  obj/src/tests/authgen/main.cpp \
  obj/src/tests/authgen/Fixture.cpp \
  obj/src/tests/authgen/Options.cpp \
  $(end)
#
$(scold_cxxdir)/tests/authgen/dependencies.mk: $(check_bin_authgen_SOURCES)
$(check_bin_authgen_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(TF_V_DIS) $(call TF_XCPP2CPP, $<)
	$(TF_V_DUN) test -e $@
$(scold_cxxdir)/tests/authgen/dependencies.mk:
	$(TF_V_COM) $(call TF_DEPENDENCIES, $^)
	$(TF_V_DUN) test -e $@
include $(scold_cxxdir)/tests/authgen/dependencies.mk
clean-dependencies::
	rm -f $(scold_cxxdir)/tests/authgen/dependencies.mk
