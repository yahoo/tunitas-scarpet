check_lib_libtests_run_la = check/lib/libtests-run.la
check_lib_libtests_run_la_LIBADD = 
check_lib_libtests_run_la_LDFLAGS = $(AND_CHECK_LDFLAGS_SET)
check_lib_libtests_run_la_MODULES = $(call TF_SOURCEStoMODULES, $(check_lib_libtests_run_la_SOURCES))
check_lib_libtests_run_la_SOURCES = \
  obj/src/tests/run/namespace.cpp \
  obj/src/tests/run/Driver.cpp \
  obj/src/tests/run/Collected.cpp \
  $(end)
#
$(scold_cxxdir)/tests/run/dependencies.mk: $(check_lib_libtests_run_la_SOURCES)
$(check_lib_libtests_run_la_SOURCES) : obj/src/%.cpp : src/%.xcpp
	$(TF_V_DIS) $(call TF_XCPP2CPP, $<)
	$(TF_V_DUN) test -e $@
$(scold_cxxdir)/tests/run/dependencies.mk:
	$(TF_V_COM) $(call TF_DEPENDENCIES, $^)
	$(TF_V_DUN) test -e $@
include $(scold_cxxdir)/tests/run/dependencies.mk
clean-dependencies::
	rm -f $(scold_cxxdir)/tests/run/dependencies.mk
#end
